  "version": "1.0.0",
  "description": "Backend for Naitai - Node.js + Express + ESM + Supabase",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "test": "vitest run",
